package com.leon.demo.designpattern.create;

public class SingleTonDemo {

}


/**
 * 单例设计模式 - 恶汉式
 * 在初始化时
 * @author leonwang
 *
 */
class SingleTon1{
	private SingleTon1 sl = new SingleTon1();
	private SingleTon1(){
		System.out.println("初始化");
	}
	public SingleTon1 getInstance(){
		return sl;
	}
}